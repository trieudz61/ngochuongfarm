{
  "name": "ngọc-hường-farm---nông-sản-sạch-tươi-ngon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"cyan,magenta\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cd server && npm start",
    "install:all": "npm install && cd server && npm install",
    "start": "npm run build && npm run server",
    "postinstall": "cd server && npm install",
    "deploy:hosting": "node deploy-to-hosting.js",
    "deploy:backend": "cd server && npm start",
    "update:api": "node update-api-url.js"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react-router-dom": "^7.11.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@reduxjs/toolkit": "^2.11.2",
    "react-redux": "^9.2.0",
    "@google/genai": "^1.34.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "concurrently": "^8.2.2"
  }
}
